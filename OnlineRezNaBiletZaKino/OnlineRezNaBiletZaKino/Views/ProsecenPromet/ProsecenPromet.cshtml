@model IEnumerable<OnlineRezNaBiletZaKino.Models.ProsecenPrometPoMesec>

@{
    ViewBag.Title = "ProsecenPromet";
}

<h2>Месечен просечен промет на киното</h2>

<table class="table">
    <tr>
        <th>
            Месец
        </th>
        <th>
            Година
        </th>
        <th>
            Просечен промет
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Mesec)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Godina)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Prosecen_promet)
    </td>
</tr>
}

</table>
